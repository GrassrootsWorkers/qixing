<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dream.qixing.persistence.interfaces.vender.IVenderDao" >
    
 <select id="findVenderByVenderId" resultType="vender" parameterType="java.lang.String" >
       select
       		*
       from tb_vender
       where venderId = #{venderId}
    </select>
</mapper>